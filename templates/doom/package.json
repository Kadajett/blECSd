{
	"name": "blecsd-template-doom",
	"version": "0.0.1",
	"description": "Doom source port rendered in the terminal via Kitty graphics protocol",
	"private": true,
	"type": "module",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsup index.ts --format esm --dts",
		"start": "node dist/index.js",
		"dev": "tsx index.ts",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"blecsd": "^0.1.0"
	},
	"devDependencies": {
		"tsup": "^8.5.1",
		"tsx": "^4.19.3",
		"typescript": "^5.8.2"
	}
}
